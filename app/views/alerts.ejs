<%- include header.ejs %>
<div class="container">
	<div class="row">
		<div class="col-lg-12">
			<table class="table table-hover table-striped">
				<!-- <tr><th>Name</th><th>Temperature</th><th>Humidity</th><th>Luminosity</th><th>Last Com</th></tr> -->
				<% for(var k=0; k < alerts_count; k++) { %>
				<tr class="<%= alerts[k].acknowledgedBy ? '' : 'danger' %>">
					<td><a href="/systems/systemDetails?uid=<%= alerts[k].target %>"><%= alerts[k].targetDescription.name %></a></td>
					<td><%= alerts[k].rule.message ? alerts[k].rule.message : alerts[k].rule.name %></td>
					<td><%= alerts[k].acknowledgedBy ? alerts[k].acknowledgedBy : '' %></td>
					<td><%= new Date( alerts[k].date ).toString() %></td>
				</tr>
				<% } %>
			</table>
		</div>
	</div>
</div>
<%- include footer.ejs %>
